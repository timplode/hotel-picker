    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "billingAddressee": {
      "type": "string"
    },
    "billingCity": {
      "type": "string"
    },
    "billingCountry": {
      "type": "string"
    },
    "billingStreet1": {
      "type": "string"
    },
    "billingStreet2": {
      "type": "string"
    },
    "confirmationNumbers": {
      "type": "json"
    },
    "contactCell": {
      "type": "string"
    },
    "contactEmail": {
      "type": "string"
    },
    "contactFirstName": {
      "type": "string"
    },
    "contactLastName": {
      "type": "string"
    },
    "notesForHotel": {
      "type": "text"
    },
    "owner": {
      "type": "string"
    },
    "primary": {
      "type": "string"
    },
    "requiresBusParking": {
      "type": "boolean",
